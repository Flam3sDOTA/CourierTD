	 <script>
	 
	 </script>
    
    <Panel hittest="false" class="Root">
        <Panel hittest="false" id="ErrorPanel">
            <Label id="ErrorMessage" text="" />
        </Panel>
        <Panel hittest="false" class="TopLeftMenu">
			<Button id="NewDashboardButton" class="TopLeftMenuButton" onactivate="DOTAHUDShowDashboard();" onmouseover="UIShowTextTooltip(DOTA_HUD_BackToDashboard)"  onmouseout="UIHideTextTooltip()" />
			<Button id="NewSettingsButton" class="TopLeftMenuButton" onactivate="DOTAShowSettingsPopup();" onmouseover="UIShowTextTooltip(DOTA_HUD_Settings)"  onmouseout="UIHideTextTooltip()" />
			<Button id="LeaderboardButton" class="TopLeftMenuButton" onactivate="OnLeaderboardOpenButtonPressed()" onload="DOTAShowTextTooltip('Leaderboards - Soon')" onmouseover="UIShowTextTooltip(Leaderboards - Soon)"  onmouseout="UIHideTextTooltip()" />
			<Button id="NewDiscordButton" class="TopLeftMenuButton" onactivate="ExternalBrowserGoToURL('https://discord.gg/WPmtWyB');" onmouseover="UIShowTextTooltip(Join the Discord Server)"  onmouseout="UIHideTextTooltip()" />
		</Panel>

		<Panel id="PlayerDeathInformationPanel">
            <Label id="PlayerDeathInformationPanelTitle" text="#Summary" />
			<Panel class="PlayerDeathInformationPanelTop">
				<Label id="PlayerDeathInformationPanelMMRTitle" text="MMR Change:" />
				<Label id="PlayerDeathInformationPanelMMRChange" text="+25" />
			</Panel>
            <Panel class="PlayerDeathInformationPanelBottom">
                <Button id="WatchButton" onactivate="WatchButtonClicked()">
                    <Label text="#Watch"/>
                </Button>
                <Button id="QuitButton" onactivate="DOTAHUDShowDashboard();">
                    <Label text="#Quit"/>
                </Button>
            </Panel>
        </Panel>

		<Panel hittest="false" id="RankedSeason0PopupWindow">
			<Panel id="BackgroundImage"/>
			<Panel id="Contents">
				<Label id="Title" text="Ranked Season 0 is Live"/>
				<Label id="Message" text="12 August 2021 - 01 October 2021"/>
			</Panel>
			<Panel id="Buttons" class="PopupButtonRow" onactivate="RankedSeason0PopupWindowButtonClicked()">
				<TextButton class="ButtonBevel" text="#DOTA_Continue"/>
			</Panel>
    	</Panel>

		<Panel hittest="false" class="TopInformationPanelRoot">
			<Panel hittest="false" class="TopInformatioPanelTop">
				<Panel hittest="false" class="TopInformatioPanelTopLeft">
					<Label id="RoundNumber" text="#Wave" />
					<Label id="RoundNumberLabel" text="1" />
				</Panel>
				<Panel hittest="false" class="TopInformatioPanelTopCenter">
					<Panel hittest="false" class="TopInformatioPanelTopCenterLeft">
						<Panel class="RoundNameIcon" onmouseover="UIShowTextTooltip('Unity Type')"  onmouseout="UIHideTextTooltip()"/>
						<Panel class="UnitCountIcon" onmouseover="UIShowTextTooltip('Unit Count')"  onmouseout="UIHideTextTooltip()"/>
					</Panel>
					<Panel hittest="false" class="TopInformatioPanelTopCenterRight">
						<Label id="RoundNameLabel" class="RoundNameLabel" text="Normal" />
						<Label id="UnitCountLabel" class="UnitCountLabel" text="40x" />
					</Panel>
				</Panel>
				<Panel hittest="false" class="TopInformatioPanelTopRight">
					<Label id="NextRound" class="NextRound" text="Next Round in:" />
					<Label id="NextRoundLabel" class="NextRoundLabel" text="00:29" />
				</Panel>
			</Panel>
			<Panel hittest="false" class="TopInformatioPanelBottom">
				<Label id="Timer" class="Timer" text="00:00:00" />
			</Panel>
		</Panel>
		
